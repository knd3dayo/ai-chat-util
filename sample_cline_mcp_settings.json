{
    "mcpServers": {
        "AIChatUtil": {
            "timeout": 60,
            "type": "stdio",
            "command": "uv",
            "args": [
                "--directory",
                "<PATH_TO_VENV>",
                "run",
                "-m",
                "ai_chat_util.mcp.mcp_server"
            ],
            "env": {
                "LLM_PROVIDER": "openai",
                "OPENAI_API_KEY": "sk-****",
                "OPENAI_COMPLETION_MODEL": "gpt-4.1",
                "OPENAI_EMBEDDING_MODEL": "text-embedding-3-small",
                "USE_CUSTOM_PDF_ANALYZER": "true"
            }
        }
    }
}